    <path>Aircraft/La-5/Models/Interior/Panel/Instruments/MagneticCompass/compas.xml</path>
    <offsets>
      <x-m> -0.006 </x-m>
      <y-m>  0.057 </y-m>
      <z-m>  0.099 </z-m>
    </offsets>
