<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>panel.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>panel</object-name>

    <!-- Objets transparents -->
  </animation>

  <!-- Clock -->
  <model>
    <path>Aircraft/La-5/Models/Interior/Panel/Instruments/clock/clock.xml</path>
    <offsets>
      <x-m> -0.001 </x-m>
      <y-m> -0.145 </y-m>
      <z-m>  0.146 </z-m>
    </offsets>
  </model>

  <!-- Asi -->
  <model>
    <path>Aircraft/La-5/Models/Interior/Panel/Instruments/asi/asi.xml</path>
    <offsets>
      <x-m> -0.005 </x-m>
      <y-m> -0.080 </y-m>
      <z-m>  0.140 </z-m>
    </offsets>
  </model>

